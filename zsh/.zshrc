: 'Load myfunction files.' && {
  source ./.zsh.d/utils.zsh
}

: 'Load zplug files' && {
  source ./.zsh.d/zplug.zsh
}

: 'Load alias files.' && {
  source ./.zsh.d/alias.zsh
}

: 'Load environment files.' && {
  source ./.zsh.d/env.zsh
}

: 'Load prompt files.' && {
  source ./.zsh.d/prompt.zsh
}

: 'Load zsh option files.' && {
  source ./.zsh.d/option.zsh
}

: 'Load zsh completion files.' && {
  source ./.zsh.d/completion.zsh
}
